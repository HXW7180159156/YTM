{
	"name": "ytm-auto-mcu-toolchain-install",
	"displayName": "ytm-auto-mcu-toolchain-install",
	"description": "The extension will help to install all the required toolchains to develop application software/codes for YTMicro's YTM32 automotive-grade MCU portfolio, including CMake, Ninja, GNU/GCC, debug for ARM Cortex-M.",
	"version": "0.0.1",
	"publisher": "YTMicro",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-sample",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.ytmInstallToolchain",
				"title": "ytm"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2"
	}
}
